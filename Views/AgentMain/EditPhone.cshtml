@model AURA.Models.AgentsPhone

@{
    ViewData["Title"] = "EditPhone";
}

<h1>EditPhone</h1>

<h4>AgentsPhone</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="EditPhone">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="UserId" class="control-label"></label>
                <input asp-for="UserId" class="form-control" readonly="readonly" />
                <span asp-validation-for="UserId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PhoneNumber" class="control-label"></label>
                <input asp-for="PhoneNumber" class="form-control" />
                <span asp-validation-for="PhoneNumber" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CountryCode" class="control-label"></label>
                <input asp-for="CountryCode" class="form-control" />
                <span asp-validation-for="CountryCode" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CarrierName" class="control-label"></label>
                <input asp-for="CarrierName" class="form-control" />
                <span asp-validation-for="CarrierName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="EmailText" class="control-label"></label>
                <input asp-for="EmailText" class="form-control" />
                <span asp-validation-for="EmailText" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Notes" class="control-label"></label>
                <input asp-for="Notes" class="form-control" />
                <span asp-validation-for="Notes" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="AgentDetail" asp-route-UserId="@Model.UserId">Back to Detail</a>
</div>

