@model AURA.Models.PostFou

@{
    
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>FouDelete</title>
</head>
<body>
    <div>
        <strong>CONTRACT</strong>--<a asp-action="SendContract" asp-route-id="@Model.FouId" asp-route-address="@Model.FouPhon" asp-route-zero="@Model.FouZero" asp-route-type="phone">TEXT</a>|<a asp-action="SendContract" asp-route-id="@Model.FouId" asp-route-address="@Model.FouEmai" asp-route-zero="@Model.FouZero" asp-route-type="email">EMAIL</a>
      </div>
    @*<div>
        <strong>MATERIAL CHANGE</strong>--<a asp-action="FouDelete" asp-route-id="@Model.FouId" asp-route-phone="@Model.FouPhon" asp-route-type="MatChg">TEXT</a>|<a asp-action="FouDelete" asp-route-id="@Model.FouId" asp-route-email="@Model.FouEmai" asp-route-type="MatChg">EMAIL</a>

    </div>*@
    <div>
        <strong style="color:red">@ViewBag.Status</strong>
    </div>
    <hr />

    <a asp-action="FouEdit" asp-route-id="@Model.FouId">EDIT</a> |
    <a asp-action="PostDetail" asp-route-zero="@Model.FouZero">BACK TO DETAIL</a>

    <div>

        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.FouZero)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.FouZero)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.FouDigit)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.FouDigit)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.FouName)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.FouName)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.FouPhon)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.FouPhon)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.FouEmai)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.FouEmai)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.FouAddr)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.FouAddr)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.FouPost)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.FouPost)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.FouOrg)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.FouOrg)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.FouNote)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.FouNote)
            </dd>
        </dl>

        <form asp-action="FouDelete">
            <input type="hidden" asp-for="FouId" />
            <input type="submit" value="Delete" class="btn btn-danger" /> |
            <a asp-action="PostDetail" asp-route-zero="@Model.FouZero">BACK TO DETAIL</a>
        </form>
    </div>
</body>
</html>
