    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EigZero)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EigDigit)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EigAgen)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EigCont)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EigRole)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EigLoad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EigNote)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.EigId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.EigId">Details</a> |
