@model IEnumerable<AURA.Models.PostEig>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.EigZero)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EigDigit)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EigAgen)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EigCont)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EigRole)
